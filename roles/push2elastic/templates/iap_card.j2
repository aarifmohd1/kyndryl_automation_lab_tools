{
    "@type": "MessageCard",
    "@context": "http://schema.org/extensions",
    "themeColor": "0076D7",
    "summary": "process automation notification",
    "sections": [{
        "activityTitle": "KAL stats",
        "activitySubtitle": "Kyndryl Automation Lab statistics",
        "activityImage": "data:image/png;base64,",
        "facts": [{
            "name": "Statistics data since",
            "value": "{{ date_since | d('beginning of time') }}"
        },{
            "name": "Source instance",
            "value": "{{ tower_host }}"
        },{
            "name": "Logged in users",
            "value": "{{ users_count }}"
        },{
            "name": "Created projects",
            "value": "{{ projects_count}}"
        },{
            "name": "Number of assets",
            "value": "{{ projects_urls | unique | length }}"
        },{
            "name": "Number of jobs",
            "value": "{{ jobs_count }}"
        },{
            "name": "Purposes of tests",
            "value": "Code development: {{ purposes_list_counted['code development'] }}, Education: {{ purposes_list_counted['education'] }}, Test of deployment: {{ purposes_list_counted['test of deployment'] }}"
        },{
            "name": "Assets list",
            "value": "{{ projects_urls | unique | join(', ') }}"
        }],
        "potentialAction": [
            {
                "@context": "http://schema.org",
                "@type": "ViewAction",
                "name": "View in Ansible Tower",
                "target": [
                    "{{ tower_host }}/{{ tower_job_suffix }}"
                ]
            }
        ],
        "markdown": true
    }]
}
